{
  "$id": "urn:no:nav:fpdokgen:etterlys-inntektsmelding",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "description": "Dokumentdata for brev etterlys inntektsmelding",
  "type": "object",
  "required": [
    "felles",
    "søknadDato",
    "antallIkkeMottattIM",
    "antallMottattIM",
    "inntektsmeldingerStatus"
  ],
  "$defs": {
    "arbeidsforholdInntektsmelding": {
      "type": "object",
      "required": [
        "arbeidsgiverIdent",
        "arbeidsgiverNavn",
        "stillingsprosent",
        "erInntektsmeldingMottatt"
      ],
      "properties": {
        "arbeidsgiverIdent": {
          "type": "string"
        },
        "arbeidsgiverNavn": {
          "type": "string"
        },
        "stillingsprosent": {
          "type": "number"
        },
        "erInntektsmeldingMottatt": {
          "type": "boolean"
        }
      }
    }
  },
  "properties": {
    "felles": {
      "$ref": "classpath:content/templates/schema_felles.json"
    },
    "søknadDato": {
      "type": "string"
    },
    "antallIkkeMottattIM": {
      "type": "number"
    },
    "antallMottattIM": {
      "type": "number"
    },
    "inntektsmeldingerStatus": {
      "type": "array",
      "items": { "$ref": "#/$defs/arbeidsforholdInntektsmelding" }
    }
  }
}
