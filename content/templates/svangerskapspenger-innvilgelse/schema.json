{
  "$id": "svangerskapspenger-innvilgelse",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "description": "Dokumentdata for brev svangerskapspenger innvilgelse",
  "type": "object",
  "required": [
    "felles",
    "revurdering",
    "endretFraAvslag",
    "utbetalingEndret",
    "termindatoEndret",
    "refusjonTilBruker",
    "antallRefusjonerTilArbeidsgivere",
    "månedsbeløp",
    "klagefristUker",
    "antallUttaksperioder",
    "uttaksaktiviteter",
    "utbetalingsperioder",
    "inkludereBeregning"
  ],
  "$defs": {
    "uttaksperiode": {
      "type": "object",
      "required": [
        "periodeFom",
        "periodeTom",
        "utbetalingsgrad"
      ],
      "properties": {
        "periodeFom": {
          "type": "string"
        },
        "periodeTom": {
          "type": "string"
        },
        "utbetalingsgrad": {
          "type": "number"
        }
      }
    },
    "uttaksaktivitet": {
      "type": "object",
      "required": [
        "aktivitetsbeskrivelse",
        "uttaksperioder"
      ],
      "properties": {
        "aktivitetsbeskrivelse": {
          "type": "string"
        },
        "uttaksperioder": {
          "type": "array",
          "items": { "$ref": "#/$defs/uttaksperiode" }
        }
      }
    },
    "utbetalingsperiode": {
      "type": "object",
      "required": [
        "periodeFom",
        "periodeTom",
        "periodeDagsats",
        "utbetaltTilSøker"
      ],
      "properties": {
        "periodeFom": {
          "type": "string"
        },
        "periodeTom": {
          "type": "string"
        },
        "periodeDagsats": {
          "type": "number"
        },
        "utbetaltTilSøker": {
          "type": "number"
        }
      }
    },
    "avslagsperiode": {
      "type": "object",
      "required": [
        "periodeFom",
        "periodeTom",
        "årsak"
      ],
      "properties": {
        "periodeFom": {
          "type": "string"
        },
        "periodeTom": {
          "type": "string"
        },
        "årsak": {
          "type": "string"
        },
        "arbeidsforholdInformasjon": {
          "type": "object",
          "properties":{
            "arbeidsgivernavn": {
              "type": "string"
            },
            "aktivitetType": {
              "type": "string"
            }
          }
        }
      }
    },
    "avslåttAktivitet": {
      "type": "object",
      "required": [
        "årsak",
        "erFL",
        "erSN"
      ],
      "properties": {
        "årsak": {
          "type": "string"
        },
        "arbeidsgiverNavn": {
          "type": "string"
        },
        "erFL": {
          "type": "boolean"
        },
        "erSN": {
          "type": "boolean"
        }
      }
    },
    "arbeidsforhold": {
      "type": "object",
      "required": [
        "arbeidsgiverNavn",
        "månedsinntekt"
      ],
      "properties": {
        "arbeidsgiverNavn": {
          "type": "string"
        },
        "månedsinntekt": {
          "type": "number"
        }
      }
    },
    "selvstendigNæringsdrivende": {
      "type": "object",
      "required": [
        "nyoppstartet",
        "årsinntekt",
        "sistLignedeÅr",
        "inntektLavere_AT_SN",
        "inntektLavere_AT_FL_SN",
        "inntektLavere_FL_SN"
      ],
      "properties": {
        "nyoppstartet": {
          "type": "boolean"
        },
        "årsinntekt": {
          "type": "number"
        },
        "sistLignedeÅr": {
          "type": "number"
        },
        "inntektLavere_AT_SN": {
          "type": "boolean"
        },
        "inntektLavere_AT_FL_SN": {
          "type": "boolean"
        },
        "inntektLavere_FL_SN": {
          "type": "boolean"
        }
      }
    },
    "frilanser": {
      "type": "object",
      "required": [
        "månedsinntekt"
      ],
      "properties": {
        "månedsinntekt": {
          "type": "number"
        }
      }
    },
    "naturalytelse": {
      "type": "object",
      "required": [
        "status",
        "endringsdato",
        "nyDagsats",
        "arbeidsgiverNavn"
      ],
      "properties": {
        "status": {
          "type": "string",
          "enum": [ "TILKOMMER", "BORTFALLER" ]
        },
        "endringsdato": {
          "type": "string"
        },
        "nyDagsats": {
          "type": "number"
        },
        "arbeidsgiverNavn": {
          "type": "string"
        }
      }
    }
  },
  "properties": {
    "felles": {
      "$ref": "file:content/templates/schema_felles.json"
    },
    "revurdering": {
      "type": "boolean"
    },
    "endretFraAvslag": {
      "type": "boolean"
    },
    "utbetalingEndret": {
      "type": "boolean"
    },
    "termindatoEndret": {
      "type": "boolean"
    },
    "refusjonTilBruker": {
      "type": "boolean"
    },
    "antallRefusjonerTilArbeidsgivere": {
      "type": "number"
    },
    "stønadsperiodeTom": {
      "type": "string"
    },
    "månedsbeløp": {
      "type": "number"
    },
    "mottattDato": {
      "type": "string"
    },
    "klagefristUker": {
      "type": "number"
    },
    "antallUttaksperioder": {
      "type": "number"
    },
    "uttaksaktiviteter": {
      "type": "array",
      "items": { "$ref": "#/$defs/uttaksaktivitet" }
    },
    "utbetalingsperioder": {
      "type": "array",
      "items": { "$ref": "#/$defs/utbetalingsperiode" }
    },
    "avslagsperioder": {
      "type": "array",
      "items": { "$ref": "#/$defs/avslagsperiode" }
    },
    "avslåtteAktiviteter": {
      "type": "array",
      "items": { "$ref": "#/$defs/avslåttAktivitet" }
    },
    "inkludereBeregning": {
      "type": "boolean"
    },
    "arbeidsforhold": {
      "type": "array",
      "items": { "$ref": "#/$defs/arbeidsforhold" }
    },
    "selvstendigNæringsdrivende": {
      "$ref": "#/$defs/selvstendigNæringsdrivende"
    },
    "frilanser": {
      "$ref": "#/$defs/frilanser"
    },
    "naturalytelser": {
      "type": "array",
      "items": { "$ref": "#/$defs/naturalytelse" }
    },
    "bruttoBeregningsgrunnlag": {
      "type": "number"
    },
    "militærSivil": {
      "type": "boolean"
    },
    "inntektOver6G": {
      "type": "boolean"
    },
    "seksG": {
      "type": "number"
    },
    "lovhjemmel": {
      "type": "string"
    }
  }
}
