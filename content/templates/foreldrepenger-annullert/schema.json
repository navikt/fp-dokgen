  "description": "Dokumentdata for brev Foreldrepenger annullert",
  "type": "object",
  "required": [
    "felles",
    "harSøktOmNyPeriode",
    "klagefristUker"
  ],
  "properties": {
    "felles": {
      "$ref": "file:content/templates/schema_felles.json"
    },
    "harSøktOmNyPeriode": {
      "type": "boolean"
    },
    "planlagtOppstartDato": {
      "type": "string"
    },
    "kanBehandlesDato": {
      "type": "string"
    },
    "klagefristUker": {
      "type": "number"
    },
    "mottarPleiepenger": {
      "type": "boolean"
    }
  }
}
