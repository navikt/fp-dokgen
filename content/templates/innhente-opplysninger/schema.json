{
  "$id": "innhente-opplysninger",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "description": "Dokumentdata for brev Innhente opplysninger",
  "type": "object",
  "required": [
    "felles",
    "førstegangsbehandling",
    "revurdering",
    "endringssøknad",
    "død",
    "klage"
  ],
  "properties": {
    "felles": {
      "$ref": "file:content/templates/schema_felles.json"
    },
    "førstegangsbehandling": {
      "type": "boolean"
    },
    "revurdering": {
      "type": "boolean"
    },
    "endringssøknad": {
      "type": "boolean"
    },
    "død": {
      "type": "boolean"
    },
    "klage": {
      "type": "boolean"
    },
    "søknadDato": {
      "type": "string"
    },
    "fristDato": {
      "type": "string"
    }
  }
}
