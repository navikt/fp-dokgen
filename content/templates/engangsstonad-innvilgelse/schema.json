{
  "$id": "engangsstonad-innvilgelse",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "description": "Dokumentdata for brev Engangsstønad innvilgelse",
  "type": "object",
  "required": [
    "felles",
    "revurdering",
    "førstegangsbehandling",
    "medhold",
    "innvilgetBeløp",
    "klagefristUker",
    "død",
    "fbEllerMedhold",
    "erEndretSats"
  ],
  "properties": {
    "felles": {
      "$ref": "file:content/templates/schema_felles.json"
    },
    "revurdering": {
      "type": "boolean"
    },
    "førstegangsbehandling": {
      "type": "boolean"
    },
    "medhold": {
      "type": "boolean"
    },
    "innvilgetBeløp": {
      "type": "string"
    },
    "klagefristUker": {
      "type": "number"
    },
    "død": {
      "type": "boolean"
    },
    "fbEllerMedhold": {
      "type": "boolean"
    },
    "erEndretSats": {
      "type": "boolean"
    }
  }
}
