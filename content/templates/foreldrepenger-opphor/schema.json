{
  "$id": "foreldrepenger-opphor",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "description": "Dokumentdata for brev Foreldrepenger Opphør",
  "type": "object",
  "required": [
    "felles",
    "erSøkerDød",
    "relasjonskode",
    "antallBarn",
    "antallÅrsaker",
    "avslagÅrsaker",
    "klagefristUker",
    "lovhjemmelForAvslag"
  ],
  "properties": {
    "felles": {
      "$ref": "file:content/templates/schema_felles.json"
    },
    "erSøkerDød": {
      "type": "boolean"
    },
    "relasjonskode": {
      "type": "string"
    },
    "fomStønadsdato": {
      "type": "string"
    },
    "tomStønadsdato": {
      "type": "string"
    },
    "opphørDato": {
      "type": "string"
    },
    "barnDødsdato": {
      "type": "string"
    },
    "gjelderFødsel": {
      "type": "boolean"
    },
    "antallBarn": {
      "type": "number"
    },
    "halvG": {
      "type": "number"
    },
    "antallÅrsaker": {
      "type": "number"
    },
    "avslagÅrsaker": {
      "type": "array",
      "items": { "type": "string" }
    },
    "klagefristUker": {
      "type": "number"
    },
    "lovhjemmelForAvslag": {
      "type": "string"
    }
  }
}
