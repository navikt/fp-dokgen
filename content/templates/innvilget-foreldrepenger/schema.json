{
  "$id": "http://nav.no/foreldrepenger/fpdokgen/innvilget-foreldrepenger",
  "$schema": "http://json-schema.org/draft/2020-12/schema",
  "description": "Dokumentdata for brev Innvilget Foreldrepenger",
  "type": "object",
  "required": [
    "felles",
    "behandlingType",
    "behandlingResultatType",
    "konsekvensForInnvilgetYtelse",
    "søknadsdato",
    "dekningsgrad",
    "dagsats",
    "månedsbeløp",
    "seksG",
    "inntektOverSeksG",
    "forMyeUtbetalt",
    "inntektMottattArbeidsgiver",
    "annenForelderHarRett",
    "annenForelderHarRettVurdert",
    "aleneomsorgKode",
    "ikkeOmsorg",
    "barnErFødt",
    "årsakErFødselshendelse",
    "gjelderMor",
    "gjelderFødsel",
    "erBesteberegning",
    "ingenRefusjon",
    "delvisRefusjon",
    "fullRefusjon",
    "antallPerioder",
    "harInnvilgedePerioder",
    "antallArbeidsgivere",
    "dagerTaptFørTermin",
    "disponibleDager",
    "disponibleFellesDager",
    "sisteDagAvSistePeriode",
    "stønadsperiodeFom",
    "stønadsperiodeTom",
    "foreldrepengeperiodenUtvidetUker",
    "antallBarn",
    "prematurDager",
    "perioder",
    "bruttoBeregningsgrunnlag",
    "harBruktBruttoBeregningsgrunnlag",
    "beregningsgrunnlagregler",
    "klagefristUker",
    "inkludereInfoOmUtbetaling",
    "inkludereUtbetaling",
    "inkludereInnvilget",
    "inkludereAvslag"
  ],
  "$defs": {
    "arbeidsforhold": {
      "type": "object",
      "properties": {
        "arbeidsgiverNavn": {
          "type": "string"
        },
        "gradering": {
          "type": "boolean"
        },
        "uttaksgrad": {
          "type": "number"
        },
        "prosentArbeid": {
          "type": "number"
        },
        "stillingsprosent": {
          "type": "number"
        },
        "utbetalingsgrad": {
          "type": "number"
        },
        "naturalytelseEndringType": {
          "type": { "enum": [ "START", "STOPP", "INGEN_ENDRING" ] }
        },
        "naturalytelseEndringDato": {
          "type": "string"
        },
        "naturalytelseNyDagsats": {
          "type": "number"
        }
      }
    },
    "næring": {
      "type": "object",
      "properties": {
        "gradering": {
          "type": "boolean"
        },
        "uttaksgrad": {
          "type": "number"
        },
        "prosentArbeid": {
          "type": "number"
        },
        "sistLignedeÅr": {
          "type": "number"
        }
      }
    },
    "annenAktivitet": {
      "type": "object",
      "properties": {
        "aktivitetStatus": {
          "type": "string"
        },
        "gradering": {
          "type": "boolean"
        },
        "uttaksgrad": {
          "type": "number"
        },
        "prosentArbeid": {
          "type": "number"
        }
      }
    },
    "utbetalingsperiode": {
      "type": "object",
      "properties": {
        "innvilget": {
          "type": "boolean"
        },
        "årsak": {
          "type": "string"
        },
        "periodeFom": {
          "type": "string"
        },
        "periodeTom": {
          "type": "string"
        },
        "periodeDagsats": {
          "type": "number"
        },
        "antallTapteDager": {
          "type": "number"
        },
        "arbeidsforholdsliste": {
          "type": "array",
          "items": { "$ref": "#/$defs/arbeidsforhold" }
        },
        "næring": { "$ref": "#/$defs/næring" },
        "annenAktivitetsliste": {
          "type": "array",
          "items": { "$ref": "#/$defs/annenAktivitet" }
        }
      }
    },
    "beregningsgrunnlagAndel": {
      "type": "object",
      "properties": {
        "aktivitetStatus": {
          "type": "string"
        },
        "arbeidsgiverNavn": {
          "type": "string"
        },
        "dagsats": {
          "type": "number"
        },
        "månedsinntekt": {
          "type": "number"
        },
        "årsinntekt": {
          "type": "number"
        },
        "etterlønnSluttpakke": {
          "type": "boolean"
        }
      }
    },
    "beregningsgrunnlagRegel": {
      "type": "object",
      "properties": {
        "aktivitetStatus": {
          "type": "string"
        },
        "antallArbeidsgivereIBeregningUtenEtterlønnSluttpakke": {
          "type": "int"
        },
        "snNyoppstartet": {
          "type": "boolean"
        },
        "andelListe": {
          "type": "array",
          "items": { "$ref": "#/$defs/beregningsgrunnlagAndel" }
        }
      }
    }
  },
  "properties": {
    "felles": {
      "type": "object"
    },
    "behandlingType": {
      "type": "string"
    },
    "behandlingResultatType": {
      "type": "string"
    },
    "konsekvensForInnvilgetYtelse": {
      "type": { "enum": [ "ENDRING_I_BEREGNING", "ENDRING_I_UTTAK", "ENDRING_I_BEREGNING_OG_UTTAK" ] }
    },
    "søknadsdato": {
      "type": "string"
    },
    "dekningsgrad": {
      "type": "number"
    },
    "dagsats": {
      "type": "number"
    },
    "månedsbeløp": {
      "type": "number"
    },
    "seksG": {
      "type": "number"
    },
    "inntektOverSeksG": {
      "type": "number"
    },
    "forMyeUtbetalt": {
      "type": { "enum": [ "FERIE", "JOBB", "GENERELL" ] }
    },
    "inntektMottattArbeidsgiver": {
      "type": "boolean"
    },
    "annenForelderHarRett": {
      "type": "boolean"
    },
    "annenForelderHarRettVurdert": {
      "type": "boolean"
    },
    "aleneomsorgKode": {
      "type": { "enum": [ "JA", "NEI", "IKKE_VURDERT" ] }
    },
    "ikkeOmsorg": {
      "type": "boolean"
    },
    "barnErFødt": {
      "type": "boolean"
    },
    "årsakErFødselshendelse": {
      "type": "boolean"
    },
    "gjelderMor": {
      "type": "boolean"
    },
    "gjelderFødsel": {
      "type": "boolean"
    },
    "erBesteberegning": {
      "type": "boolean"
    },
    "ingenRefusjon": {
      "type": "boolean"
    },
    "delvisRefusjon": {
      "type": "boolean"
    },
    "fullRefusjon": {
      "type": "boolean"
    },
    "antallPerioder": {
      "type": "number"
    },
    "harInnvilgedePerioder": {
      "type": "boolean"
    },
    "antallArbeidsgivere": {
      "type": "number"
    },
    "dagerTaptFørTermin": {
      "type": "number"
    },
    "disponibleDager": {
      "type": "number"
    },
    "disponibleFellesDager": {
      "type": "number"
    },
    "sisteDagAvSistePeriode": {
      "type": "string"
    },
    "innvilgetBeløp": {
      "type": "string"
    },
    "stønadsperiodeFom": {
      "type": "string"
    },
    "stønadsperiodeTom": {
      "type": "string"
    },
    "foreldrepengeperiodenUtvidetUker": {
      "type": "number"
    },
    "antallBarn": {
      "type": "number"
    },
    "prematurDager": {
      "type": "number"
    },
    "perioder": {
      "type": "array",
      "items": { "$ref": "#/$defs/utbetalingsperiode" }
    },
    "bruttoBeregningsgrunnlag": {
      "type": "number"
    },
    "harBruktBruttoBeregningsgrunnlag": {
      "type": "boolean"
    },
    "beregningsgrunnlagregler": {
      "type": "array",
      "items": { "$ref": "#/$defs/beregningsgrunnlagRegel" }
    },
    "klagefristUker": {
      "type": "number"
    },
    "inkludereInfoOmUtbetaling": {
      "type": "boolean"
    },
    "inkludereUtbetaling": {
      "type": "boolean"
    },
    "inkludereInnvilget": {
      "type": "boolean"
    },
    "inkludereAvslag": {
      "type": "boolean"
    }
  }
}
