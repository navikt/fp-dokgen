{
  "$id": "innvilget-foreldrepenger",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "description": "Dokumentdata for brev Innvilget Foreldrepenger",
  "type": "object",
  "required": [
    "felles",
    "behandlingType",
    "behandlingResultatType",
    "konsekvensForInnvilgetYtelse",
    "søknadsdato",
    "dekningsgrad",
    "dagsats",
    "månedsbeløp",
    "seksG",
    "inntektOverSeksG",
    "inntektMottattArbeidsgiver",
    "annenForelderHarRett",
    "annenForelderHarRettVurdert",
    "aleneomsorgKode",
    "ikkeOmsorg",
    "barnErFødt",
    "årsakErFødselshendelse",
    "gjelderMor",
    "gjelderFødsel",
    "erBesteberegning",
    "ingenRefusjon",
    "delvisRefusjon",
    "fullRefusjon",
    "fbEllerRvInnvilget",
    "antallPerioder",
    "antallInnvilgedePerioder",
    "antallAvslåttePerioder",
    "antallArbeidsgivere",
    "dagerTaptFørTermin",
    "disponibleDager",
    "sisteDagAvSistePeriode",
    "antallBarn",
    "antallDødeBarn",
    "perioder",
    "bruttoBeregningsgrunnlag",
    "harBruktBruttoBeregningsgrunnlag",
    "beregningsgrunnlagregler",
    "klagefristUker",
    "lovhjemlerUttak",
    "inkludereUtbetaling",
    "inkludereUtbetNårGradering",
    "inkludereInnvilget",
    "inkludereAvslag",
    "inkludereNyeOpplysningerUtbet"
  ],
  "$defs": {
    "arbeidsforhold": {
      "type": "object",
      "required": [
        "arbeidsgiverNavn",
        "gradering",
        "prosentArbeid",
        "stillingsprosent",
        "utbetalingsgrad"
      ],
      "properties": {
        "arbeidsgiverNavn": {
          "type": "string"
        },
        "gradering": {
          "type": "boolean"
        },
        "prosentArbeid": {
          "type": "number"
        },
        "stillingsprosent": {
          "type": "number"
        },
        "utbetalingsgrad": {
          "type": "number"
        },
        "naturalytelseEndringType": {
          "type": "string",
          "enum": [ "START", "STOPP", "INGEN_ENDRING" ]
        },
        "naturalytelseEndringDato": {
          "type": "string"
        },
        "naturalytelseNyDagsats": {
          "type": "number"
        }
      }
    },
    "næring": {
      "type": "object",
      "required": [
        "gradering",
        "prosentArbeid",
        "utbetalingsgrad"
      ],
      "properties": {
        "gradering": {
          "type": "boolean"
        },
        "utbetalingsgrad": {
          "type": "number"
        },
        "prosentArbeid": {
          "type": "number"
        }
      }
    },
    "annenAktivitet": {
      "type": "object",
      "required": [
        "aktivitetStatus",
        "gradering",
        "utbetalingsgrad",
        "prosentArbeid"
      ],
      "properties": {
        "aktivitetStatus": {
          "type": "string"
        },
        "gradering": {
          "type": "boolean"
        },
        "utbetalingsgrad": {
          "type": "number"
        },
        "prosentArbeid": {
          "type": "number"
        }
      }
    },
    "utbetalingsperiode": {
      "type": "object",
      "required": [
        "innvilget",
        "årsak",
        "periodeFom",
        "periodeTom",
        "periodeDagsats",
        "antallTapteDager",
        "prioritertUtbetalingsgrad"
      ],
      "properties": {
        "innvilget": {
          "type": "boolean"
        },
        "årsak": {
          "type": "string"
        },
        "periodeFom": {
          "type": "string"
        },
        "periodeTom": {
          "type": "string"
        },
        "periodeDagsats": {
          "type": "number"
        },
        "antallTapteDager": {
          "type": "number"
        },
        "prioritertUtbetalingsgrad": {
          "type": "number"
        },
        "arbeidsforholdsliste": {
          "type": "array",
          "items": { "$ref": "#/$defs/arbeidsforhold" }
        },
        "næring": { "$ref": "#/$defs/næring" },
        "annenAktivitetsliste": {
          "type": "array",
          "items": { "$ref": "#/$defs/annenAktivitet" }
        }
      }
    },
    "beregningsgrunnlagAndel": {
      "type": "object",
      "required": [
        "aktivitetStatus",
        "dagsats",
        "månedsinntekt",
        "årsinntekt"
      ],
      "properties": {
        "aktivitetStatus": {
          "type": "string"
        },
        "arbeidsgiverNavn": {
          "type": "string"
        },
        "dagsats": {
          "type": "number"
        },
        "månedsinntekt": {
          "type": "number"
        },
        "årsinntekt": {
          "type": "number"
        },
        "etterlønnSluttpakke": {
          "type": "boolean"
        },
        "sistLignedeÅr": {
          "type": "number"
        }
      }
    },
    "beregningsgrunnlagRegel": {
      "type": "object",
      "required": [
        "regelStatus",
        "antallArbeidsgivereIBeregningUtenEtterlønnSluttpakke",
        "snNyoppstartet"
      ],
      "properties": {
        "regelStatus": {
          "type": "string"
        },
        "antallArbeidsgivereIBeregningUtenEtterlønnSluttpakke": {
          "type": "number"
        },
        "snNyoppstartet": {
          "type": "boolean"
        },
        "andelListe": {
          "type": "array",
          "items": { "$ref": "#/$defs/beregningsgrunnlagAndel" }
        }
      }
    }
  },
  "properties": {
    "felles": {
      "$ref": "file:content/templates/schema_felles.json"
    },
    "behandlingType": {
      "type": "string"
    },
    "behandlingResultatType": {
      "type": "string"
    },
    "konsekvensForInnvilgetYtelse": {
      "type": "string"
    },
    "søknadsdato": {
      "type": "string"
    },
    "dekningsgrad": {
      "type": "number"
    },
    "harUtbetaling": {
      "type": "boolean"
    },
    "dagsats": {
      "type": "number"
    },
    "månedsbeløp": {
      "type": "number"
    },
    "seksG": {
      "type": "number"
    },
    "inntektOverSeksG": {
      "type": "boolean"
    },
    "forMyeUtbetalt": {
      "type": "string",
      "enum": [ "FERIE", "JOBB", "GENERELL" ]
    },
    "inntektMottattArbeidsgiver": {
      "type": "boolean"
    },
    "annenForelderHarRett": {
      "type": "boolean"
    },
    "annenForelderHarRettVurdert": {
      "type": "string",
      "enum": [ "JA", "NEI", "IKKE_VURDERT" ]
    },
    "aleneomsorgKode": {
      "type": "string",
      "enum": [ "JA", "NEI", "IKKE_VURDERT" ]
    },
    "ikkeOmsorg": {
      "type": "boolean"
    },
    "barnErFødt": {
      "type": "boolean"
    },
    "årsakErFødselshendelse": {
      "type": "boolean"
    },
    "gjelderMor": {
      "type": "boolean"
    },
    "gjelderFødsel": {
      "type": "boolean"
    },
    "erBesteberegning": {
      "type": "boolean"
    },
    "ingenRefusjon": {
      "type": "boolean"
    },
    "delvisRefusjon": {
      "type": "boolean"
    },
    "fullRefusjon": {
      "type": "boolean"
    },
    "fbEllerRvInnvilget": {
      "type": "boolean"
    },
    "antallPerioder": {
      "type": "number"
    },
    "antallInnvilgedePerioder": {
      "type": "number"
    },
    "antallAvslåttePerioder": {
      "type": "number"
    },
    "antallArbeidsgivere": {
      "type": "number"
    },
    "dagerTaptFørTermin": {
      "type": "number"
    },
    "disponibleDager": {
      "type": "number"
    },
    "disponibleFellesDager": {
      "type": "number"
    },
    "sisteDagAvSistePeriode": {
      "type": "string"
    },
    "innvilgetBeløp": {
      "type": "string"
    },
    "stønadsperiodeFom": {
      "type": "string"
    },
    "stønadsperiodeTom": {
      "type": "string"
    },
    "foreldrepengeperiodenUtvidetUker": {
      "type": "number"
    },
    "antallBarn": {
      "type": "number"
    },
    "prematurDager": {
      "type": "number"
    },
    "antallDødeBarn": {
      "type": "number"
    },
    "dødsdato": {
      "type": "string"
    },
    "bruttoBeregningsgrunnlag": {
      "type": "number"
    },
    "perioder": {
      "type": "array",
      "items": { "$ref": "#/$defs/utbetalingsperiode" }
    },
    "harBruktBruttoBeregningsgrunnlag": {
      "type": "boolean"
    },
    "beregningsgrunnlagregler": {
      "type": "array",
      "items": { "$ref": "#/$defs/beregningsgrunnlagRegel" }
    },
    "klagefristUker": {
      "type": "number"
    },
    "lovhjemlerUttak": {
      "type": "string"
    },
    "lovhjemlerBeregning": {
      "type": "string"
    },
    "inkludereUtbetaling": {
      "type": "boolean"
    },
    "inkludereUtbetNårGradering": {
      "type": "boolean"
    },
    "inkludereInnvilget": {
      "type": "boolean"
    },
    "inkludereAvslag": {
      "type": "boolean"
    },
    "inkludereNyeOpplysningerUtbet": {
      "type": "boolean"
    }
  }
}
