{{/if}}{{/if}}{{/if}}{{/if}}
{{#if arbeidsforholdsliste.0.gradering}} {{#if (and (eq this.utbetalingsgrad 100.0) (eq this.gradering false))}}
  I same perioden tek du ut 100 prosent permisjon hos {{this.arbeidsgiverNavn}}.
{{/if}}{{/if}}
{{/each}}
{{/if}}


