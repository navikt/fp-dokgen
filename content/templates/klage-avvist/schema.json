{
  "$id": "klage-avvist",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "description": "Dokumentdata for brev klage avvist",
  "type": "object",
  "required": [
    "felles",
    "gjelderTilbakekreving",
    "lovhjemler",
    "klagefristUker",
    "antallGrunner",
    "avvistGrunner"
  ],
  "properties": {
    "felles": {
      "$ref": "file:content/templates/schema_felles.json"
    },
    "gjelderTilbakekreving": {
      "type": "boolean"
    },
    "lovhjemler": {
      "type": "string"
    },
    "klagefristUker": {
      "type": "number"
    },
    "antallGrunner": {
      "type": "number"
    },
    "avvistGrunner": {
      "type": "array",
      "items": { "type": "string" }
    }
  }
}
